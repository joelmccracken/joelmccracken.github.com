---
layout: nil
---
<feed xmlns="http://www.w3.org/2005/Atom">
 
 <title>Joel McCracken</title>
 <link href="http://joelmccracken.github.com/atom.xml" rel="self"/>
 <link href="http://joelmccracken.github.com/"/>
 <updated>{{ site.time | date_to_xmlschema }}</updated>
 <id>http://joelmccracken.github.com/</id>
 <author>
   <name>Joel McCracken</name>
   <email>jom24g@gmail.com</email>
 </author>
 
 {% for post in site.posts %}
 <entry>
   <title>{{ post.title }}</title>
   <link href="http://joelmccracken.github.com{{ post.url }}"/>
   <updated>{{ post.date | date_to_xmlschema }}</updated>
   <id>http://joelmccracken.github.com{{ post.id }}</id>
   <content type="html">
     {{ post.content | xml.escape }}
   </content>
 </entry>
 {% endfor %}
 
</feed>
